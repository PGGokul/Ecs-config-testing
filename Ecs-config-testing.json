{
	"family": "Ecs-config-testing",
	"taskRoleArn": "arn:aws:iam::013424092965:role/task_role",
	"containerDefinitions": [{
		"name": "Ecs-config-testing",
		"image": "013424092965.dkr.ecr.ap-south-1.amazonaws.com/Ecs-config-testing:GIT_COMMIT",
		"cpu": 500,
		"memory": 650,
		"memoryReservation": 450,
		"volumesFrom": [],
		"portMappings": [{
			"hostPort": 0,
			"containerPort": 4000,
			"protocol": "tcp"
		}],
		"essential": true,
		"logConfiguration": {
			"logDriver": "awslogs",
			"options": {
			  "awslogs-group": "GROUP_NAME",
			  "awslogs-region": "ap-south-1",
			  "awslogs-stream-prefix": "Ecs-config-testing"
			}
		  },
		"environment": [
			{
				"name": "START_CMD",
				"value": "startup_cmd"
			}
		],
		"mountPoints": []
	}]
}
